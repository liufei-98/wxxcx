.index-header-wrap{
  width: 96%;
  height: 200rpx;
  margin: 40rpx 2%;
  border-radius: 20rpx;
  background-color: #fff;
  display: flex;
  justify-content: start;
  align-items: center;
}
.index-header-goods{
  width: 20%;
  text-align: center;
}
.index-header-goods image{
  width: 100rpx;
  height: 100rpx;
}
.index-header-goods .catName{
  font-size: 24rpx;
  text-align: center;
}

.index-brand-wrap{
  width: 96%;
  margin: 0 2% 20rpx;
  border-radius: 20rpx;
  background-color: #fff;
  display: flex;
  flex-wrap: wrap;
  justify-content: start;
  align-items: center;
}
.index-brand-wrap .index-brand-goods{
  width: 25%;
  height: 140rpx;
  text-align: center;
  margin-bottom: 20rpx;
}
.index-brand-wrap .index-brand-goods image{
  width: 136rpx;
  height: 60rpx;
  margin: 20rpx 0;
}
.index-brand-wrap .index-brand-goods .brandName{
  font-size: 24rpx;
  text-align: center;
}
.index-content-wrap{
  width: 96%;
  margin: 0 2%;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  justify-content: space-between;
}
.index-content-wrap .index-content-goods{
  width: 49%;
  height: 520rpx;
  margin: 20rpx 0 0;
  border-radius: 20rpx;
  float: left;
  background: #fff;
  overflow: hidden;
}
.index-content-wrap .index-content-goods image{
  width: 100%;
  height: 320rpx;
}
.index-content-wrap .index-content-goods .title{
  width: 90%;
  margin: 0 5%;
  font-size: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.index-content-wrap .index-content-goods .price{
  width: 90%;
  margin: 20rpx 5% 0;
  color: #f44;
  font-weight: 900;
}
.member-swipe-wrap{
  width: 100%;
  height: 400rpx;
  display: flex;
  flex-wrap: wrap;
  align-items: baseline;
  background: #fff;
}
.member-swipe-wrap .member-list{
  width: 20%;
  height: 180rpx;
}
.member-swipe-wrap .member-list image{
  width: 136rpx;
  height: 136rpx;
}
.member-swipe-wrap .member-list .member-title{
  font-size: 24rpx;
  text-align: center;
}
.swiper4{
  height: 60rpx;
}
.seckill-goods-wrap{
  height:400rpx;
  display: flex;
}
.seckill-goods-wrap .skillGoods{
  width: 240rpx;
  height: 400rpx;
}
.seckill-goods-wrap .skillGoods image{
  width: 230rpx;
  height: 230rpx;
}
.seckill-goods-wrap .skillGoods .goodsInfo{
  width: 100%;
  height: 110rpx;
  margin: 20rpx 10rpx;
  display: flex;
  flex-wrap: wrap;
}
.seckill-goods-wrap .skillGoods .goodsInfo .goodsName{
  font-size: 26rpx;
  width: 100%;
  text-overflow: ellipsis;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  text-align: center;
}
.seckill-goods-wrap .skillGoods .goodsInfo .secPrice{
  width: 100%;
  font-size: 30rpx;
  color: red;
  font-weight: 700;
  text-align: center;
}
.seckill-goods-wrap .skillGoods .goodsInfo .marketPrice{
  width: 100%;
  font-size: 21rpx;
  color: #ccc;
  text-decoration: line-through;
  text-align: center;
}
.adimage-wrap{
  width: 96%;
  height: 200rpx;
  margin: 18rpx 2% 0;
}
.adimage-wrap image{
  width: 100%;
  height: 100%;
}
.content-category-wrap{
  width: 100%;
  height: 100%;
}
.content-category-wrap .little-category{
  width: 100%;
  display: flex;
  justify-content: start;
  flex-wrap: wrap;
}
.content-category-wrap .little-category .little-category-title{
  width: 100%;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  font-size: 26rpx;
  position: relative;
  margin: 22rpx 0;
}
.content-category-wrap .little-category .little-category-title::before{
  content: "";
  width: 30rpx;
  height: 2rpx;
  background: #e0e0e0;
  position: absolute;
  top: 24rpx;
  left: 195rpx;
}
.content-category-wrap .little-category .little-category-title::after{
  content: "";
  width: 30rpx;
  height: 2rpx;
  background: #e0e0e0;
  position: absolute;
  top: 24rpx;
  right: 195rpx;
}
.content-category-wrap .little-category .little-category-goods{
  width: 33%;
  height: 160rpx;
  margin-bottom: 40rpx;
  position: relative;
}
.content-category-wrap .little-category .little-category-goods image{
  width: 104rpx;
  height: 104rpx;
  position: absolute;
  top: 0;
  left: 40rpx;
}
.content-category-wrap .little-category .little-category-goods view{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 56rpx;
  text-align: center;
  line-height: 56rpx;
  font-size: 24rpx;
  color: #777;
}